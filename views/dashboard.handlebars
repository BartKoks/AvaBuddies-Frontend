<div class="d-sm-flex align-items-center justify-content-between mb-4">
    <h1 class="h3 mb-0 text-gray-800">{{{parameters.title}}}</h1>
</div>

<div class="col-12">
    <table class="table table-striped table-dark">
      <thead>
        <tr>
          <th scope="col">Naam</th>
          <th scope="col">Auteur</th>
          <th scope="col">Aantal kroegen</th>
        </tr>
      </thead>
      <tbody>
        {{#each users}}
          <tr>
            <td>{{name}}</td>
              <td>{{email}}</td>
            <td>
              <a href="/races/{{email}}" class="btn btn-primary"><i class="fas fa-edit"></i></a>
              <button data-id="{{email}}" type="button" class="btn btn-danger delete"><i class="fas fa-trash-alt"></i></button>
            </td>
          </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
  
